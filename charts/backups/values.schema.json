        "restart": {
          "default": "Never",
          "description": "Set the cronjob parameter restart",
          "enum": [
            "Never",
            "OnFailure"
          ],
          "required": [],
          "title": "restart"
        },
        "schedule": {
          "default": "@daily",
          "description": "Set the cronjob parameter schedule",
          "required": [],
