                        },
                        "concurrencyPolicy": {
                            "type": "string"
                        },
                        "failedJobsHistoryLimit": {
                            "type": "integer"
                        },
                        "restartPolicy": {
                            "type": "string"
                        },
                        "schedule": {
                            "type": "string"
