{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "cloudflared": {
            "type": "object",
            "properties": {
                "account": {
                    "type": "string"
                },
                "affinity": {
                    "type": "object"
                },
                "autoscaling": {
                    "type": "object",
                    "properties": {
                        "behavior": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                },
                                "scaleDown": {
                                    "type": "object"
                                },
                                "scaleUp": {
                                    "type": "object"
                                }
                            }
                        },
                        "customMetrics": {
                            "type": "array"
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "maxReplicas": {
                            "type": "integer"
                        },
                        "minReplicas": {
                            "type": "integer"
                        },
                        "targetCPUUtilizationPercentage": {
                            "type": "integer"
                        },
                        "targetMemoryUtilizationPercentage": {
                            "type": "null"
                        }
                    }
                },
                "dnsConfig": {
                    "type": "object"
                },
                "enableWarp": {
                    "type": "boolean"
                },
                "enabled": {
                    "type": "boolean"
                },
                "extraEnv": {
                    "type": "array"
                },
                "extraEnvFrom": {
                    "type": "array"
                },
                "extraVolumeMounts": {
                    "type": "array"
                },
                "extraVolumes": {
                    "type": "array"
                },
                "hostAliases": {
                    "type": "array"
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "pullPolicy": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "imagePullSecrets": {
                    "type": "array"
                },
                "ingress": {
                    "type": "array"
                },
                "livenessProbe": {
                    "type": "object",
                    "properties": {
                        "failureThreshold": {
                            "type": "integer"
                        },
                        "httpGet": {
                            "type": "object",
                            "properties": {
                                "path": {
                                    "type": "string"
                                },
                                "port": {
                                    "type": "integer"
                                }
                            }
                        },
                        "initialDelaySeconds": {
                            "type": "integer"
                        },
                        "periodSeconds": {
                            "type": "integer"
                        }
                    }
                },
                "maxUnavailable": {
                    "type": "null"
                },
                "nodeSelector": {
                    "type": "object"
                },
                "podAnnotations": {
                    "type": "object"
                },
                "podLabels": {
                    "type": "object"
                },
                "podSecurityContext": {
                    "type": "object"
                },
                "readinessProbe": {
                    "type": "object",
                    "properties": {
                        "httpGet": {
                            "type": "object",
                            "properties": {
                                "path": {
                                    "type": "string"
                                },
                                "port": {
                                    "type": "integer"
                                }
                            }
                        }
                    }
                },
                "replicaCount": {
                    "type": "integer"
                },
                "resources": {
                    "type": "object"
                },
                "secret": {
                    "type": "string"
                },
                "securityContext": {
                    "type": "object",
                    "properties": {
                        "allowPrivilegeEscalation": {
                            "type": "boolean"
                        },
                        "capabilities": {
                            "type": "object",
                            "properties": {
                                "drop": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        },
                        "readOnlyRootFilesystem": {
                            "type": "boolean"
                        }
                    }
                },
                "tolerations": {
                    "type": "array"
                },
                "tunnelId": {
                    "type": "string"
                },
                "tunnelName": {
                    "type": "string"
                }
            }
        },
        "cloudflaredSidecars": {
            "type": "array"
        },
        "cloudflaredSplitter": {
            "type": "object",
            "properties": {
                "beams": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                },
                "extraEnv": {
                    "type": "array"
                },
                "extraEnvFrom": {
                    "type": "array"
                },
                "extraVolumeMounts": {
                    "type": "array"
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "pullPolicy": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "livenessProbe": {
                    "type": "object"
                },
                "readinessProbe": {
                    "type": "object"
                },
                "resources": {
                    "type": "object"
                },
                "securityContext": {
                    "type": "object"
                }
            }
        },
        "fullnameOverride": {
            "type": "string"
        },
        "nameOverride": {
            "type": "string"
        },
        "namespaceOverride": {
            "type": "string"
        },
        "pinniped": {
            "type": "object",
            "properties": {
                "concierge": {
                    "type": "object",
                    "properties": {
                        "credentialIssuerConfig": {
                            "type": "string"
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "jwtAuths": {
                            "type": "object"
                        },
                        "teams": {
                            "type": "object"
                        }
                    }
                },
                "enabled": {
                    "type": "boolean"
                },
                "supervisor": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "federationDomains": {
                            "type": "object"
                        },
                        "githubProviders": {
                            "type": "object"
                        }
                    }
                }
            }
        },
        "revisionHistoryLimit": {
            "type": "integer"
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "automount": {
                    "type": "boolean"
                },
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "serviceMonitor": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                },
                "interval": {
                    "type": "null"
                },
                "labels": {
                    "type": "object"
                },
                "matchExpressions": {
                    "type": "array"
                },
                "metricRelabelings": {
                    "type": "array"
                },
                "namespace": {
                    "type": "null"
                },
                "namespaceSelector": {
                    "type": "object"
                },
                "relabelings": {
                    "type": "array"
                },
                "scheme": {
                    "type": "string"
                },
                "scrapeTimeout": {
                    "type": "null"
                },
                "targetLabels": {
                    "type": "array"
                },
                "tlsConfig": {
                    "type": "null"
                }
            }
        }
    }
}
